{
  "name": "vuerd-vscode",
  "version": "0.7.8",
  "displayName": "ERD Editor",
  "description": "ERD Editor vscode extension",
  "main": "./out/extension.js",
  "private": true,
  "icon": "img/vuerd.png",
  "publisher": "dineug",
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "vuerd",
    "ERD",
    "Editor",
    "vscode",
    "db",
    "database",
    "diagram"
  ],
  "homepage": "https://github.com/vuerd/vuerd-vscode#readme",
  "bugs": {
    "url": "https://github.com/vuerd/vuerd-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vuerd/vuerd-vscode.git"
  },
  "author": "dineug <dineug2@gmail.com>",
  "license": "MIT",
  "engines": {
    "vscode": "^1.47.0"
  },
  "activationEvents": [
    "onCommand:vuerd.webview",
    "onWebviewPanel:vuerd.webview",
    "onCustomEditor:vuerd.editor"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "vuerd.editor",
        "displayName": "vuerd",
        "selector": [
          {
            "filenamePattern": "*.vuerd"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "vuerd",
        "extensions": [
          ".vuerd"
        ],
        "aliases": [
          "vuerd"
        ]
      }
    ],
    "commands": [
      {
        "command": "vuerd.webview",
        "title": "Open ERD Editor",
        "category": "vuerd",
        "icon": {
          "light": "./img/vuerd.png",
          "dark": "./img/vuerd.png"
        }
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vuerd.webview",
          "when": "resourceFilename =~ /.*\\.(vuerd.json)/",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "vuerd.webview",
          "when": "resourceFilename =~ /.*\\.(vuerd.json)/",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "command": "vuerd.webview",
          "when": "resourceFilename =~ /.*\\.(vuerd.json)/",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "vuerd.webview",
          "when": "resourceFilename =~ /.*\\.(vuerd.json)/",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "vuerd",
